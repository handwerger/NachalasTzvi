<script type="text/javascript">
 var playing = false;

 function playshiur(playerId, mediaSrc, headphonesId, listenId){
 
  if (playing==false){
 document.getElementById(playerId).innerHTML='<audio id="homepageplayer1" class="player"  controls="on" autoplay><source src="'+mediaSrc+'"></audio>';
 document.getElementById(headphonesId).title="close player";
 document.getElementById(listenId).innerHTML="Close Player";
 document.getElementById(listenId).style = "text-decoration:underline";
 document.getElementById(listenId).title="close player";
 playing = true;
}
  else {
  /*document.getElementById("homepageplayer1").pause();*/
  document.getElementById(playerId).innerHTML="";
  document.getElementById(headphonesId).title="listen";
  document.getElementById(listenId).innerHTML="Listen";
  document.getElementById(listenId).style = "text-decoration:underline";
  document.getElementById(listenId).title = "listen";
  playing=false;
 }
}

</script>
